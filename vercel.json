{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "app/api/upload/route.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/**/*.js": {
      "maxDuration": 30,
      "memory": 512
    }
  }
}
